{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "npm run lint -- --fix",
    "format:fix": "npx prettier . --write",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "cd .. && husky install next-app/.husky"
  },
  "type": "module",
  "dependencies": {
    "@next/eslint-plugin-next": "^14.0.3",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "next": "14.0.3",
    "next-redux-wrapper": "^8.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^8.1.3"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "msw": "^2.0.9",
    "prettier": "3.1.0",
    "typescript": "^5",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "/*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run format:fix"
    ]
  }
}
